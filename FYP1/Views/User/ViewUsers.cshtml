@{
ViewData["Title"] = "View Users";
Layout = "~/Views/Shared/_Layout.cshtml";

}


<!--begin::Card-->
<div class="card card-custom">
    <div class="card-header flex-wrap border-0 pt-6 pb-0">
        <div class="card-title">
            <h3 class="card-label">@ViewData["Title"]
            </h3>
        </div>
    </div>
    <div class="card-body">
        <!--begin: Datatable-->
        <table class="table table-bordered table-hover table-checkable mt-10" id="viewuser">
            <thead>
                <tr>
                    <th>Username</th>
                    <th>Full Name</th>
                    <th>CNIC</th>
                    <th>Email</th>
                    <th>Role</th>
                    <th>Status</th>
                    <th>Start Date</th>
                    <th>Actions</th>
                </tr>
            </thead>
        </table>
        <!--end: Datatable-->
    </div>
</div>
<!--end::Card-->

<script src="~/LMS/lib/jquery-3.6.0.js"></script>
<script src="~/LMS/lib/jquery.validate.min.js"></script>
<script type="text/javascript" src="~/LMS/ViewsJquery/ViewUsers.js"></script>