@{
ViewData["Title"] = "Calender";
Layout = "~/Views/Shared/_Layout.cshtml";

}
<!--begin::Card-->
<div class="card card-custom">
  <div class="card-header">
    <div class="card-title">
      <h3 class="card-label">@ViewData["Title"]</h3>
    </div>

  </div>
  <div class="card-body">
    <table class="table table-bordered table-hover table-checkable mt-10" id="_schedule">
      <thead>
        <tr>
          <th>Day</th>
          <th>Time</th>
          <th>Course</th>
          <th>Class Id</th>
        </tr>
      </thead>
    </table>
  </div>
</div>
<!--end::Card-->
<script src="~/LMS/lib/jquery-3.6.0.js"></script>
<script src="~/LMS/lib/jquery.validate.min.js"></script>
<script type="text/javascript" src="~/LMS/ViewsJquery/AdminCalender.js"></script>