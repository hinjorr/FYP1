@{
ViewData["Title"] = "AddNewDpt";
Layout = "~/Views/Shared/_Layout.cshtml";

}
<div class="row mt">
    <div class="col-lg-4">
        <div class="form-panel">
            <h4 class="mb"><i class="fa fa-angle-right"></i> Add new Program</h4>
            <form>
                <div class="form-group row">
                    <div class="col-sm-12">
                        <label>Program Full Name
                            <span class="error">*</span>
                        </label>
                        <input type="text" class="form-control" placeholder="" id="txtFullName">
                    </div>
                    <div class="col-sm-12">
                        <label>Program Short Name
                            <span class="error">*</span>
                        </label>
                        <input type="text" class="form-control" placeholder="" id="txtShortName">
                    </div>
                </div>
                <button type="button" class="btn btn-primary btn-lg btn-block" id="btnCreate">Create</button>
            </form>
        </div>
    </div>
    <div class="col-lg-8">
